{
  "name": "8puzzle",
  "version": "0.0.1",
  "description": "A node-based version of an 8-puzzle solver project.",
  "main": "solver.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src -d ./build --copy-files",
    "clean": "rm -rf ./build",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix",
    "prestart": "yarn build",
    "start": "node ./build/index.js ../puzzles/testPuzzle.txt",
    "poststart": "yarn clean",
    "pretest": "yarn build",
    "test": "./node_modules/.bin/mocha ./build/**/*.test.js -G --recursive",
    "posttest": "yarn clean"
  },
  "author": "Matt Bengston [https://bengsfort.github.io/portfolio-2018] <bengston.matthew@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bengsfort/javascript-8-puzzle.git"
  },
  "bugs": {
    "url": "https://github.com/bengsfort/javascript-8-puzzle/issues"
  },
  "homepage": "https://github.com/bengsfort/javascript-8-puzzle#readme",
  "dependencies": {
    "chai": "^4.0.0",
    "flow-bin": "^0.47.0",
    "microtime": "^2.1.8",
    "mocha": "^3.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-plugin-flowtype": "^3.0.0"
  }
}
